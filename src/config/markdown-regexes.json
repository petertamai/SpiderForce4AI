{
    "double_empty_lines": "\\n\\s*\\n",
    "line_breaks_before_headers": "([^\\n])(\\s*#+\\s+.*)",
    "markdown_links_or_images": "^\\s*#+\\s*\\[.*?\\]\\(.*?\\)\\s*$",
    "list_items_followed_by_bracket": "(\\* .*)\\n(\\s*\\[.*?\\]\\(.*?\\))",
    "adjacent_links": "(\\))(\\S)",
    "headers_with_markdown_links": "^\\s*#+\\s*\\[.*?\\]\\(.*?\\)\\s*$",
    "markdown_links_starting_with_hash": "\\[(.*?)\\]\\(#.*?\\)",
    "empty_headers": "^\\s*#+\\s*$",
    "extra_lines": "\\n{3,}",
    "empty_hash": "(^\\s*#\\s*$\\n)+",
    "empty_markdown_links": "\\[\\s*\\]\\((.*?)\\)",
    "content_followed_by_dashes": "^(.*\\S.*)(\\n\\s*-{2,}\\s*$)",
    "clean_links": "\\[\\s*([^\\]]+?)\\s*\\]\\((http[^\\)]+?)\\)",
    "remove_asterisks": "^\\s*\\*\\s*$",
    "fix_missing_bracket": "^(?!\\[)(.*?)\\]\\((http[^\\)]+?)\\)",
    "remove_outer_link": "\\[[^\\]]+\\s+\\[([^\\]]+)\\]\\(([^)]+)\\)",
    "link_followed_by_text_and_equals": "(\\[.*?\\]\\(.*?\\))\\s+(.*?)\\n(={3,})$",
    "relative_url_link": "\\[?\\s*\\]?\\((brands\\/[^)]+)\\)",
    
    "remove_placeholder_images": "!\\[.*?\\]\\([^)]*(?:blank\\.gif|placeholder|spacer|empty|transparent|pixel|cache|cdn-cgi|wp-fastest-cache|lazy-load)[^)]*\\)\\s*",
    "remove_1x1_images": "!\\[.*?\\]\\([^)]*1x1[^)]*\\)\\s*",
    "remove_single_exclamation": "^\\s*!\\s*$",
    "remove_empty_image_tags": "!\\[\\]\\([^)]*\\)\\s*",
    "replace_placeholder_images": "!\\[([^\\]]*)\\]\\([^)]*(?:blank[\\.-](?:gif|png|jpe?g)|placeholder|spacer|empty|transparent|pixel|cdn-cgi|wp-fastest-cache-premium|base64)[^)]*\\)",
    "fix_broken_image_links": "\\[!\\]\\(https?://[^)]+\\)",
    "clean_cdn_images": "!\\[.*?\\]\\([^)]*(?:cdn\\.shopify\\.com|cloudfront\\.net|cloudflare\\.com)/[^)]*(?:placeholder|loading|default)[^)]*\\)\\s*",
    "remove_base64_images": "!\\[.*?\\]\\(data:image[^)]*\\)\\s*",
    "remove_tracking_pixels": "!\\[.*?\\]\\([^)]*(?:tracking|pixel|analytics|stats|counter)[^)]*\\)\\s*",
    
    "remove_unclosed_brackets": "\\[+\\s*\\]+\\s*\\([^)]*$",
    "fix_multiple_brackets": "\\[{2,}|\\]{2,}",
    "remove_hash_links": "\\[#\\s*\"[^\"]*\"\\]",
    "clean_malformed_links": "\\[+[^\\]]*\\]+\\s*\\([^)]*$",
    "remove_hanging_parentheses": "\\([^)]*$",
    "clean_multiple_exclamations": "!{2,}\\[",
    "remove_empty_link_blocks": "\\[\\s*\\]\\s*\\(\\s*\\)",
    "fix_nested_brackets": "\\[\\s*\\[+\\s*\\]+\\s*\\]",
    "remove_isolated_brackets": "^\\s*\\[+\\s*$|^\\s*\\]+\\s*$"
  }